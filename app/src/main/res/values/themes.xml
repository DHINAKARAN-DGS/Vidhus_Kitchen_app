<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.ProtiumTest" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#ED3E3B</item>
        <item name="colorPrimaryVariant">#ED3E3B3</item>
        <item name="colorOnPrimary">#fff</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <item name="android:windowDisablePreview">true</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="ProgressBar" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#00BF63</item>
        <item name="colorPrimaryVariant">#00BF63</item>
        <item name="colorOnPrimary">#00BF63</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#00BF63</item>
        <item name="colorSecondaryVariant">#00BF63</item>
        <item name="colorOnSecondary">#00BF63</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">#00BF63</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.aUTHui" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#00BF63</item>
        <item name="colorPrimaryVariant">#00BF63</item>
        <item name="colorOnPrimary">#fff</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <item name="android:windowDisablePreview">true</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.ProtiumTest.Fullscreen" parent="Theme.ProtiumTest">
        <item name="android:actionBarStyle">@style/Widget.Theme.ProtiumTest.ActionBar.Fullscreen
        </item>
        <item name="android:windowActionBarOverlay">true</item>
        <item name="android:windowBackground">@null</item>
    </style>

    <style name="ThemeOverlay.ProtiumTest.FullscreenContainer" parent="">
        <item name="fullscreenBackgroundColor">@color/light_blue_600</item>
        <item name="fullscreenTextColor">@color/light_blue_A200</item>
    </style>

</resources>